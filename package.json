{
  "name": "VaaS",
  "version": "0.4.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oslabs-beta/VaaS"
  },
  "scripts": {
    "dev:server": "nodemon --watch src/server/ --watch src/server --exec 'ts-node src/server/index.ts'",
    "dev:windows": "nodemon --watch src/server/ --watch src/server --exec ts-node src/server/index.ts",
    "dev:client": "yarn build:client && vite --config vite.config.ts dev --open",
    "build": "rimraf dist && tsc -p tsconfig.prod.json && yarn build:client && yarn build:server && yarn copy-files",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/client/entry-server.tsx --outDir dist/server",
    "test": "NODE_ENV=test vitest --environment jsdom",
    "test:windows": "set NODE_ENV=test&&vitest --environment jsdom",
    "test:server": "NODE_ENV=test nodemon --watch src/server/ --watch src/server --exec 'ts-node src/server/index.ts'",
    "test:server/windows": "set NODE_ENV=test&&nodemon --watch src/server/ --watch src/server --exec ts-node src/server/index.ts",
    "test:watch": "NODE_ENV=testvitest --watch",
    "coverage": "NODE_ENV=test vitest --coverage",
    "serve": "yarn build && cross-env NODE_ENV=production yarn ts-node ./server",
    "serve:local": "vite serve",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles images/* dist/assets && copyfiles index.html dist && copyfiles -f dist/client/assets/* dist/assets",
    "prepare": "husky install",
    "postinstall": "husky install",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx, src"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.10.2",
    "@mui/lab": "^5.0.0-alpha.110",
    "@mui/material": "^5.10.2",
    "@reduxjs/toolkit": "^1.8.4",
    "@tanstack/react-query": "^4.19.0",
    "@tanstack/react-query-devtools": "^4.19.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/isomorphic-fetch": "^0.0.36",
    "autoprefixer": "^10.4.8",
    "axios": "^1.2.0",
    "compression": "1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "cypress-mongodb": "^5.1.4",
    "isomorphic-fetch": "^3.0.0",
    "material-ui-popup-state": "^5.0.4",
    "mongodb": "^6.3.0",
    "nodemon": "^2.0.19",
    "serve-static": "^1.15.0",
    "ts-node": "^10.9.1"
  },
